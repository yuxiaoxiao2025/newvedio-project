# API Configuration
VITE_API_BASE=http://localhost:8005

# Qwen AI Model API Keys (Required for real API testing)
# 请将此文件复制为 .env.local 并填入真实的API密钥
VITE_QWEN_API_KEY=your_qwen_api_key_here

# WebSocket Configuration
VITE_WS_BASE=ws://localhost:8005

# Testing Configuration
VITE_TEST_MODE=false
VITE_USE_REAL_API=false

# Performance Settings
VITE_API_TIMEOUT=30000
VITE_MAX_FILE_SIZE=314572800

# Feature Flags
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_FUSION_ANALYSIS=true
VITE_ENABLE_MUSIC_GENERATION=true